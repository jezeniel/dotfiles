background no
out_to_console yes
out_to_x no
update_interval 2.0
total_run_times 0
use_spacer none

TEXT
^fg(\#ffcb48)${nodename}^fg()      ^fg(\#ffcb48)${sysname} ${kernel} on ${machine}^fg()

 ^fg(\#ffcb48)PROCESSING^fg()
   ^fg(\#98c2c7)CPU:^fg()     ${freq_g}GHz       ${cpu}% 
   ${acpitemp}Â°C
   ^fg(\#78af78)${cpubar}

   ^fg(\#98c2c7)NAME             PID       CPU%      MEM%
   ^fg(\#e5e5e5)${top name 1} ${top pid 1}   ${top cpu 1}    ${top mem 1}
   ^fg(\#c4c4c4)${top name 2} ${top pid 2}   ${top cpu 2}    ${top mem 2}
   ^fg(\#a3a3a3)${top name 3} ${top pid 3}   ${top cpu 3}    ${top mem 3}
   ^fg(\#a3a3a3)${top name 4} ${top pid 4}   ${top cpu 4}    ${top mem 4}

 ^fg(\#ffcb48)DATA^fg()
   ^fg(\#98c2c7)RAM:^fg()     $memperc%      ^fg(\#78af78)${membar 6}^fg()
   ^fg(\#98c2c7)NAME             PID       CPU%      MEM%
   ^fg(\#e5e5e5)${top_mem name 1} ${top_mem pid 1}   ${top_mem cpu 1} 
    ${top_mem mem 1}
   ^fg(\#c4c4c4)${top_mem name 2} ${top_mem pid 2}   ${top_mem cpu 2} 
    ${top_mem mem 2}
   ^fg(\#a3a3a3)${top_mem name 3} ${top_mem pid 3}   ${top_mem cpu 3} 
    ${top_mem mem 3}
   ^fg(\#a3a3a3)${top_mem name 4} ${top_mem pid 4}   ${top_mem cpu 4} 
    ${top_mem mem 4}

   ^fg(\#98c2c7)Swap:^fg()    ${swapperc}%            ^fg(\#78af78)${swapbar 6}^fg()
   ^fg(\#98c2c7)   /:^fg()   ${fs_free_perc /}%  ${fs_free}   ^fg(\#78af78)${fs_bar 6 /}^fg()
